<%- model_class = Company -%>
<div class="page-header">
  <h1><!--<%=t '.title', :default => Company %>-->Empresa</h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><!--<%= model_class.human_attribute_name(:name) %>-->Nome</dt>
      <dd><pre class="prettyprint"><%= @company.name %></pre></dd>
      <dt class="label label-default"><!--<%= model_class.human_attribute_name(:cnpj) %>-->CNPJ</dt>
      <dd><pre class="prettyprint"><%= @company.cnpj %></pre></dd>
      <dt class="label label-default"><!--<%= model_class.human_attribute_name(:address) %>-->Endere√ßo</dt>
      <dd><pre class="prettyprint"><%= @company.address %></pre></dd>
      <dt class="label label-default"><!--<%= model_class.human_attribute_name(:phone) %>-->Telefone</dt>
      <dd><pre class="prettyprint"><%= @company.phone %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:email) %></dt>
      <dd><pre class="prettyprint"><%= @company.email %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <!--<%= link_to t('.back', :default => t("helpers.links.back")),
              companies_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_company_path(@company), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              company_path(@company),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
	-->		  
  <%= link_to t('.back', :default => 'Voltar'),
              companies_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => 'Editar'),
              edit_company_path(@company), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => 'Excluir'),
              company_path(@company),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Confirmar?')) },
              :class => 'btn btn-danger' %>
</div>

