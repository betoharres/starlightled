<%= simple_form_for(@gateway_stat) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :serial_num %>
    <%= f.input :date %>
    <%= f.input :onlineDev %>
    <%= f.input :signal %>
    <%= f.input :lqi %>
    <%= f.input :rssi %>
    <%= f.input :correlation %>
    <%= f.input :rcvPkts %>
    <%= f.input :sentPkts %>
    <%= f.input :lastReboot %>
    <%= f.input :temp %>
    <%= f.input :battery %>
    <%= f.input :vKl %>
    <%= f.input :vZigbee %>
    <%= f.input :errorsCount %>
    <%= f.input :alerts %>
    <%= f.input :acPwr %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
